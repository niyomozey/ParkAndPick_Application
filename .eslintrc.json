{
  "extends": ["airbnb", "prettier"],
  "import/extensions": {
    "js": "ignorePackages"
  },
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "mjs": true
    },
    "envs": {
      "es6": true,
      "browser": true,
      "node": true
    }
  },
  "rules": {
    "no-console": 0,
    "no-undef": "off"
  }
}
